{
  "name": "padrinks",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node expo-start.js",
    "start:clear": "node node_modules/@expo/cli/build/bin/cli start -c",
    "start:tunnel": "node node_modules/@expo/cli/build/bin/cli start --tunnel",
    "start:windows": "start-expo.bat",
    "start:windows:clear": "start-expo-clear.bat",
    "android": "node node_modules/@expo/cli/build/bin/cli run:android",
    "ios": "node node_modules/@expo/cli/build/bin/cli run:ios",
    "web": "node node_modules/@expo/cli/build/bin/cli start --web",
    "eject": "node node_modules/@expo/cli/build/bin/cli eject",
    "expo:doctor": "node node_modules/@expo/cli/build/bin/cli doctor",
    "expo:install": "node node_modules/@expo/cli/build/bin/cli install",
    "clean": "rm -rf node_modules .expo package-lock.json && npm install",
    "clean:cache": "node node_modules/@expo/cli/build/bin/cli start -c",
    "prebuild": "node node_modules/@expo/cli/build/bin/cli prebuild",
    "build:android": "node node_modules/eas-cli/bin/run build -p android --profile preview"
  },
  "dependencies": {
    "@expo/cli": "^0.25.0",
    "@react-native-async-storage/async-storage": "2.2.0",
